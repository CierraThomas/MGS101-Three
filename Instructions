mysql> create database project;
Query OK, 1 row affected (0.00 sec)

mysql> show databases
exit
[1]+  Stopped                 sudo mysql -u root -p
cierra@cierra-VirtualBox:~$ 
mysql -u root -p project < /var/www/html/101Files/ProjectStuff

mysql use project;

mysql use project;

select * from customer;

use project;

describe customer;

exit;

mysql -u root -p project </var/www/html/101Files/ProjectStuff

mysql -u root -p 
use project;
show tables;

